{
    "name": "abdelrahmanrafaat/schema-to-code",
    "description": "Converts DB schema to Laravel code (Migrations & Models)",
    "type": "library",
    "keywords" : ["laravel", "schema", "migrations", "models"],
    "require": {
        "php": "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "abdelrahmanrafaat",
            "email": "abdel.rahmanrafaat5@gmail.com"
        }
    ],
    "autoload": {
        "psr-4":{
            "Abdelrahmanrafaat\\SchemaToCode\\" : "src/",
            "Tests\\" : "tests/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Abdelrahmanrafaat\\SchemaToCode\\Provider\\SchemaToCodeServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
